@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ebs: <http://www.elsevier.com/xml/schema/rdf/EMMeTBasicSatellite-1/> .
@prefix eds: <http://www.elsevier.com/xml/schema/rdf/EMMeTDomainSatellite-1/> .
@prefix emloc: <http://data.elsevier.com/vocabulary/EMMeT/locrel/> .
@prefix emst: <http://www.elsevier.com/xml/schema/rdf/EGVSatelliteTagging-1/> .
@prefix epr: <http://www.elsevier.com/xml/schema/rdf/EMMeTProvenanceSatellite-1/> .
@prefix pav: <http://purl.org/swan/pav/provenance/> .
@prefix prism: <http://prismstandard.org/namespaces/basic/2.0/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sat: <http://www.elsevier.com/xml/schema/rdf/LDR-Satellites/Base-1/> .
@prefix semrel: <http://data.elsevier.com/EMMeT/SemRelations/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix skosxl: <http://www.w3.org/2008/05/skos-xl#> .
@prefix sksme: <http://synaptica.net/skm/subElement/> .
@prefix svf: <http://www.elsevier.com/xml/schema/grant/grant-1.2/> .

<https://data.elsevier.com/schema/edm/> a <http://www.w3.org/2002/07/owl#Ontology>;
  dcterms:created "2018-02-18";
  dcterms:description "The Enterprise Data Model (EDM) is a fundamental component of the reference architecture of the enterprise and a part of the URA (Unified reference Architecture). It sets the foundation for a data ecosystem that aims to spans the whole of Elsevier, to enable connectivity and insight, and help value to be achieved.  The EDM establish a core set of entity types for across the organisation in a consistent manner such as People and Organisations, and foundational business and product concepts such as Product, Customer. Author and Editor.  The EDM seeks only to include abstractions of the key relationships and properties, and not to represent the full complexity of all possible scenarios. The Enterprise Data Model does not include highly specialised entity types that are only of interest to a very specific domain (e.g. Bioassay), instead these are expected to be included in domain models, which are mapped to the EDM. The EDM also seeks to provide the upper ontology that connects data silos, leading towards a Linked Data view of the enterprise and enterprise data.";
  dcterms:modified "2019-07-11";
  dcterms:source "https://confluence.cbsels.com/display/arch/Enterprise+Data+Model";
  <http://purl.org/vocab/vann/preferredNamespacePrefix> "edm";
  <http://purl.org/vocab/vann/preferredNamespaceUri> "https://data.elsevier.com/schema/edm/";
  rdfs:label "Elsevier Enterprise Data Model (EDM)";
  <http://www.w3.org/2002/07/owl#priorVersion> "https://data.elsevier.com/schema/edm/1.1/";
  <http://www.w3.org/2002/07/owl#versionIRI> "https://data.elsevier.com/schema/edm/1.2/";
  <http://www.w3.org/2002/07/owl#versionInfo> "v1.2";
  <http://www.w3.org/ns/dcat#keyword> "Top ontology", "URA";
  <http://www.w3.org/ns/dcat#theme> "Architecture" .

<http://schema.org/domainIncludes> a <http://www.w3.org/2002/07/owl#AnnotationProperty>;
  <http://schema.org/domainIncludes> <http://schema.org/Property>;
  <http://schema.org/isPartOf> <http://meta.schema.org/>;
  <http://schema.org/rangeIncludes> <http://schema.org/Class>;
  rdfs:comment "Relates a property to a class that is (one of) the type(s) the property is expected to be used on.";
  rdfs:label "domainIncludes" .

<http://schema.org/Property> a <http://www.w3.org/2002/07/owl#Class>;
  <http://schema.org/isPartOf> <http://meta.schema.org/>;
  rdfs:comment "A property, used to indicate attributes and relationships of some Thing; equivalent to rdf:Property.";
  rdfs:label "Property";
  rdfs:subClassOf <http://schema.org/Intangible> .

<http://schema.org/Intangible> a <http://www.w3.org/2002/07/owl#Class> .

<http://schema.org/Class> a <http://www.w3.org/2002/07/owl#Class>;
  <http://schema.org/isPartOf> <http://meta.schema.org/>;
  rdfs:comment "A class, also often called a 'Type'; equivalent to rdfs:Class.";
  rdfs:label "Class";
  rdfs:subClassOf <http://schema.org/Intangible> .

<http://schema.org/rangeIncludes> a <http://www.w3.org/2002/07/owl#AnnotationProperty>;
  <http://schema.org/domainIncludes> <http://schema.org/Property>;
  <http://schema.org/isPartOf> <http://meta.schema.org/>;
  <http://schema.org/rangeIncludes> <http://schema.org/Class>;
  rdfs:comment "Relates a property to a class that constitutes (one of) the expected type(s) for values of the property.";
  rdfs:label "rangeIncludes" .

<https://data.elsevier.com/schema/edm/abstract> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Work>;
  rdfs:comment "The summary of the Work.";
  rdfs:label "abstract";
  rdfs:range <http://www.w3.org/2001/XMLSchema#string>;
  rdfs:subPropertyOf <https://data.elsevier.com/schema/edm/description> .

<https://data.elsevier.com/schema/edm/Work> a <http://www.w3.org/2002/07/owl#Class>;
  rdfs:comment "An article, book or other similar creative item.  A Work can take many different forms including a wide variety of articles, images, and data sets.  Works can span a wide range of sizes, from small digests, through to multi-part constructs like encyclopedias and journals. Works can be offered for sale as a part of a Product.";
  rdfs:label "Work";
  rdfs:subClassOf <https://data.elsevier.com/schema/edm/Thing> .

<https://data.elsevier.com/schema/edm/Thing> a <http://www.w3.org/2002/07/owl#Class>;
  rdfs:comment "A superclass entity for the Enterprise Data Model, not intended to be directly used as a class.";
  rdfs:label "Thing" .

<https://data.elsevier.com/schema/edm/description> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Thing>;
  rdfs:comment "A description associated with an entity.";
  rdfs:label "description";
  rdfs:range <http://www.w3.org/2001/XMLSchema#string> .

<https://data.elsevier.com/schema/edm/acronym> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Organization>;
  rdfs:comment "The acronym of an Organization.";
  rdfs:label "acronym";
  rdfs:range <http://www.w3.org/2001/XMLSchema#string>;
  rdfs:subPropertyOf <https://data.elsevier.com/schema/edm/label> .

<https://data.elsevier.com/schema/edm/Organization> a <http://www.w3.org/2002/07/owl#Class>;
  rdfs:comment "Any level of Organization engaged in many different ways from purchasing Products, involvement in creating Works either directly or indirectly - such as by providing funding or setting regulations.  An Organization may have relationships with other Organizations and with Persons.";
  rdfs:label "Organization";
  rdfs:subClassOf <https://data.elsevier.com/schema/edm/Actor> .

<https://data.elsevier.com/schema/edm/Actor> a <http://www.w3.org/2002/07/owl#Class>;
  rdfs:comment "Super-class of entities, such as Person and Organisation or a Software Agent, that may have legal rights, and/or a legal existence, and can undertake a Role.";
  rdfs:label "Actor";
  rdfs:subClassOf <https://data.elsevier.com/schema/edm/Thing> .

<https://data.elsevier.com/schema/edm/label> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Thing>;
  rdfs:comment "The label associated with the entity.";
  rdfs:label "label";
  rdfs:range <http://www.w3.org/2001/XMLSchema#string> .

<https://data.elsevier.com/schema/edm/adverseEffectOf> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/MedicalCondition>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Drug>;
  rdfs:comment "The MedicalCondition is the adverse effect or side effect of the Drug.";
  rdfs:label "adverse effect of";
  <http://www.w3.org/2002/07/owl#inverseOf> <https://data.elsevier.com/schema/edm/hasAdverseEffect> .

<https://data.elsevier.com/schema/edm/MedicalCondition> a <http://www.w3.org/2002/07/owl#Class>;
  rdfs:comment "A condition affecting the functioning of the (human) body whether mentally or physically.";
  rdfs:label "Medical Condition";
  rdfs:subClassOf <https://data.elsevier.com/schema/edm/Thing> .

<https://data.elsevier.com/schema/edm/Drug> a <http://www.w3.org/2002/07/owl#Class>;
  rdfs:comment "Any substance or combination of substances that is used or studied for some intentional pharmacological effect; administered as an aid in the diagnosis, treatment, or prevention of disease or other abnormal condition, for the relief of pain or suffering, to control or improve any physiologic or pathologic condition or that alters the function of the body.";
  rdfs:label "Drug";
  rdfs:subClassOf <https://data.elsevier.com/schema/edm/Thing> .

<https://data.elsevier.com/schema/edm/hasAdverseEffect> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Drug>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/MedicalCondition>;
  rdfs:comment "The Drug has as adverse effect (side effect) the MedicalCondition.";
  rdfs:label "has adverse effect" .

<https://data.elsevier.com/schema/edm/affiliationOf> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Organization>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Person>;
  rdfs:comment "The Organization is linked to the Person without transferring entitlements to that Person.";
  rdfs:label "affiliation of";
  <http://www.w3.org/2002/07/owl#inverseOf> <https://data.elsevier.com/schema/edm/hasAffiliation> .

<https://data.elsevier.com/schema/edm/Person> a <http://www.w3.org/2002/07/owl#Class>;
  rdfs:comment "An individual human being.  A Person can be engaged in many different ways, from using and purchasing Products, or being a potential user of Products, through being involved in the process of creating Works, having various relationships with other People and Organizations, to being a significant individual in their field.";
  rdfs:label "Person";
  rdfs:subClassOf <https://data.elsevier.com/schema/edm/Actor> .

<https://data.elsevier.com/schema/edm/hasAffiliation> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Person>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Organization>;
  rdfs:comment "The Person is linked to the Organization without transfer of entitlements from that given Organization.";
  rdfs:label "has affiliation" .

<https://data.elsevier.com/schema/edm/alternateLabel> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Thing>;
  rdfs:comment "An alternate label for the entity.";
  rdfs:label "alternate label";
  rdfs:range <http://www.w3.org/2001/XMLSchema#string>;
  rdfs:subPropertyOf <https://data.elsevier.com/schema/edm/label> .

<https://data.elsevier.com/schema/edm/alternateName> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Actor>, <https://data.elsevier.com/schema/edm/Product>;
  rdfs:comment "An alternate name for the Actor or Product.";
  rdfs:label "alternate name";
  rdfs:range <http://www.w3.org/2001/XMLSchema#string>;
  rdfs:subPropertyOf <https://data.elsevier.com/schema/edm/name> .

<https://data.elsevier.com/schema/edm/Product> a <http://www.w3.org/2002/07/owl#Class>;
  rdfs:comment "Marketable Works and Services that are offered for purchase to multiple Persons and Organizations (Customers). Products drive the revenue that enables the company to be successful.";
  rdfs:label "Product";
  rdfs:subClassOf <https://data.elsevier.com/schema/edm/Thing> .

<https://data.elsevier.com/schema/edm/name> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Actor>, <https://data.elsevier.com/schema/edm/Product>;
  rdfs:comment "The name of an Actor or Product.";
  rdfs:label "name";
  rdfs:range <http://www.w3.org/2001/XMLSchema#string>;
  rdfs:subPropertyOf <https://data.elsevier.com/schema/edm/label> .

<https://data.elsevier.com/schema/edm/associatedAnatomy> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/MedicalCondition>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/AnatomicalStructure>;
  rdfs:comment "The MedicalCondition is connected to the AnatomicalStructure.";
  rdfs:label "associated anatomy";
  <http://www.w3.org/2002/07/owl#inverseOf> <https://data.elsevier.com/schema/edm/relatedCondition> .

<https://data.elsevier.com/schema/edm/AnatomicalStructure> a <http://www.w3.org/2002/07/owl#Class>;
  rdfs:comment "Any part of a body, typically a component of an anatomical system including organs, tissues, and cells.";
  rdfs:label "Anatomical Structure";
  rdfs:subClassOf <https://data.elsevier.com/schema/edm/Thing> .

<https://data.elsevier.com/schema/edm/relatedCondition> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/AnatomicalStructure>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/MedicalCondition>;
  rdfs:comment "The AnatomicalStructure has a MedicalCondition associated with it. For instance the head is linked to headache.";
  rdfs:label "related condition" .

<https://data.elsevier.com/schema/edm/associatedWith> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Organization>,
    <https://data.elsevier.com/schema/edm/Person>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Organization>;
  rdfs:comment "The Person or Organization is linked to the Organization with transfer of entitlements from the Organization.";
  rdfs:label "associated with";
  <http://www.w3.org/2002/07/owl#inverseOf> <https://data.elsevier.com/schema/edm/hasAssociation> .

<https://data.elsevier.com/schema/edm/hasAssociation> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Organization>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Organization>,
    <https://data.elsevier.com/schema/edm/Person>;
  rdfs:comment "The Organization linked to the Organization or Person with transfer of entitlement.";
  rdfs:label "has association" .

<https://data.elsevier.com/schema/edm/authorOf> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Person>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Work>;
  rdfs:comment "The Person is the Author of the Work.";
  rdfs:label "author of";
  <http://www.w3.org/2002/07/owl#inverseOf> <https://data.elsevier.com/schema/edm/hasAuthor> .

<https://data.elsevier.com/schema/edm/hasAuthor> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Work>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Person>;
  rdfs:comment "The Work is authored by a Person.";
  rdfs:label "has author" .

<https://data.elsevier.com/schema/edm/availableOnlineDate> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Work>;
  rdfs:comment "The date the Work was made available online for the first time. The availableOnlineDate may apply to any Work including those that have not been, and may never be, formally published by a publisher such as WorkingPapers.";
  rdfs:label "available online date";
  rdfs:range <http://www.w3.org/2001/XMLSchema#dateTime>;
  rdfs:subPropertyOf <https://data.elsevier.com/schema/edm/date> .

<https://data.elsevier.com/schema/edm/date> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Thing>;
  rdfs:comment "The date associated with the entity.";
  rdfs:label "date";
  rdfs:range <http://www.w3.org/2001/XMLSchema#dateTime>;
  rdfs:subPropertyOf <http://www.w3.org/2002/07/owl#topDataProperty> .

<https://data.elsevier.com/schema/edm/awardedTo> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/GrantAward>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Organization>,
    <https://data.elsevier.com/schema/edm/Person>;
  rdfs:comment "The GrantAward is awarded to a Person or Organization.";
  rdfs:label "awarded to";
  <http://www.w3.org/2002/07/owl#inverseOf> <https://data.elsevier.com/schema/edm/hasAward> .

<https://data.elsevier.com/schema/edm/GrantAward> a <http://www.w3.org/2002/07/owl#Class>;
  rdfs:comment "An agreement where a monetary resource is issued to a person or organization by another funding person or organization to perform research activities, often for a particular project. The award document would be a work.";
  rdfs:label "Grant Award";
  rdfs:subClassOf <https://data.elsevier.com/schema/edm/Agreement> .

<https://data.elsevier.com/schema/edm/Agreement> a <http://www.w3.org/2002/07/owl#Class>;
  rdfs:comment "A negotiated and typically legally binding arrangement between Actors as to a course of action.";
  rdfs:label "Agreement";
  rdfs:subClassOf <https://data.elsevier.com/schema/edm/Thing> .

<https://data.elsevier.com/schema/edm/hasAward> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Organization>,
    <https://data.elsevier.com/schema/edm/Person>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/GrantAward>;
  rdfs:comment "The entity is awarded a GrantAward.";
  rdfs:label "has award" .

<https://data.elsevier.com/schema/edm/birthDate> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Person>;
  rdfs:comment "The date a Person was born.";
  rdfs:label "birthdate";
  rdfs:range <http://www.w3.org/2001/XMLSchema#dateTime>;
  rdfs:subPropertyOf <https://data.elsevier.com/schema/edm/date> .

<https://data.elsevier.com/schema/edm/brand> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Product>;
  rdfs:comment "The brand associated with the Product.";
  rdfs:label "brand";
  rdfs:range <http://www.w3.org/2001/XMLSchema#string>;
  rdfs:subPropertyOf <https://data.elsevier.com/schema/edm/name> .

<https://data.elsevier.com/schema/edm/commentOf> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Comment>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Work>;
  rdfs:comment "The Comment is associated with the Work or a particular segment of the Work.";
  rdfs:label "comment of";
  <http://www.w3.org/2002/07/owl#inverseOf> <https://data.elsevier.com/schema/edm/hasComment> .

<https://data.elsevier.com/schema/edm/Comment> a <http://www.w3.org/2002/07/owl#Class>;
  rdfs:comment "A Comment is a collection of written notes or highlights pertaining to a Work that are associated with particular locations in the text. Note that the term comment is used to distinguish from annotations associating content with taxonomies or classifications.";
  rdfs:label "Comment";
  rdfs:subClassOf <https://data.elsevier.com/schema/edm/Work> .

<https://data.elsevier.com/schema/edm/hasComment> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Work>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Comment>;
  rdfs:comment "The Work or a segment of the Work has an associated Comment.";
  rdfs:label "has Comment";
  <http://www.w3.org/2002/07/owl#inverseOf> <https://data.elsevier.com/schema/edm/commentOf> .

<https://data.elsevier.com/schema/edm/contextOf> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Thing>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Agreement>,
    <https://data.elsevier.com/schema/edm/Role>;
  rdfs:comment "The entity provides context for the Agreement or Role.";
  rdfs:label "context of";
  <http://www.w3.org/2002/07/owl#inverseOf> <https://data.elsevier.com/schema/edm/hasContext> .

<https://data.elsevier.com/schema/edm/Role> a <http://www.w3.org/2002/07/owl#Class>;
  rdfs:comment "A function performed by an Actor in Relation to a Work or an Activity.";
  rdfs:label "Role";
  rdfs:subClassOf <https://data.elsevier.com/schema/edm/Thing> .

<https://data.elsevier.com/schema/edm/hasContext> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Agreement>,
    <https://data.elsevier.com/schema/edm/Role>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Thing>;
  rdfs:comment "The Agreement or Role is the context for the entity.";
  rdfs:label "has context" .

<https://data.elsevier.com/schema/edm/contraIndicationOf> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/MedicalCondition>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Drug>, <https://data.elsevier.com/schema/edm/MedicalProcedure>;
  rdfs:comment "The MedicalCondition is an indication that the Drug or MedicalProcedure should not be used.";
  rdfs:label "contraindication of";
  <http://www.w3.org/2002/07/owl#inverseOf> <https://data.elsevier.com/schema/edm/hasContraIndication> .

<https://data.elsevier.com/schema/edm/MedicalProcedure> a <http://www.w3.org/2002/07/owl#Class>;
  rdfs:comment "A series of steps by which a desired medical outcome is ideally accomplished.";
  rdfs:label "Medical Procedure";
  rdfs:subClassOf <https://data.elsevier.com/schema/edm/Thing> .

<https://data.elsevier.com/schema/edm/hasContraIndication> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Drug>, <https://data.elsevier.com/schema/edm/MedicalProcedure>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/MedicalCondition>;
  rdfs:comment "The Drug or MedicalProcedure should not be used for this MedicalCondition.";
  rdfs:label "has contraindication" .

<https://data.elsevier.com/schema/edm/copyrightLine> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Work>;
  rdfs:comment "The copyright line of the Work.";
  rdfs:label "copyright line";
  rdfs:range <http://www.w3.org/2001/XMLSchema#string>;
  rdfs:subPropertyOf <http://www.w3.org/2002/07/owl#topDataProperty> .

