  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Activity>;
  rdfs:comment "The duration of an Activity.";
  rdfs:label "duration" .

<https://data.elsevier.com/schema/edm/issueLabel> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Article>;
  rdfs:comment "The Article is identifiable as having a particular Issue number or Issue name.";
  rdfs:label "issue label" .

<https://data.elsevier.com/schema/edm/Article> a <http://www.w3.org/2002/07/owl#Class>;
  rdfs:comment "A short-form Work on a particular subject or theme. Often intended for publication in a Journal.";
  rdfs:label "Article";
  rdfs:subClassOf <https://data.elsevier.com/schema/edm/Work> .

<https://data.elsevier.com/schema/edm/partOfISBN> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Chapter>;
  rdfs:comment "The ISBN of the Book the Chapter is part of. Note: If the entity is a Book use edm:isbn. This attribute provides a shortcut without including the Book as entity associated through the edm:partOf relation.";
