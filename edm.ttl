  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Service>,
    <https://data.elsevier.com/schema/edm/Substance>, <https://data.elsevier.com/schema/edm/Thing>,
    <https://data.elsevier.com/schema/edm/Work>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Activity>,
    <https://data.elsevier.com/schema/edm/Drug>, <https://data.elsevier.com/schema/edm/Product>,
    <https://data.elsevier.com/schema/edm/Thing>;
  rdfs:comment "The entity of a given type is included in another entity of a given type. For example: a Work is included in a Product, a Substance is included in a Drug.";
  rdfs:label "included in";
  <http://www.w3.org/2002/07/owl#inverseOf> <https://data.elsevier.com/schema/edm/includes> .

<https://data.elsevier.com/schema/edm/Service> a <http://www.w3.org/2002/07/owl#Class>;
  rdfs:comment "A function or capability that can be performed. A Service can be offered for sale as a part of a Product.";
  rdfs:label "Service";
  rdfs:subClassOf <https://data.elsevier.com/schema/edm/Thing> .

<https://data.elsevier.com/schema/edm/Substance> a <http://www.w3.org/2002/07/owl#Class>;
  rdfs:comment "A substance is a form of matter having broadly consistent chemical composition and characteristic properties whose origin may be biological, mineral or chemical.";
  rdfs:label "Substance";
  rdfs:subClassOf <https://data.elsevier.com/schema/edm/Thing> .

<https://data.elsevier.com/schema/edm/includes> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Activity>,
    <https://data.elsevier.com/schema/edm/Drug>, <https://data.elsevier.com/schema/edm/Product>,
    <https://data.elsevier.com/schema/edm/Thing>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Service>,
    <https://data.elsevier.com/schema/edm/Substance>, <https://data.elsevier.com/schema/edm/Thing>,
    <https://data.elsevier.com/schema/edm/Work>;
  rdfs:comment "The entity includes other types of entity(ies). For example: a Product includes Works or Services; Drug includes Substances.";
  rdfs:label "includes";
  <http://www.w3.org/2002/07/owl#inverseOf> <https://data.elsevier.com/schema/edm/includedIn> .

<https://data.elsevier.com/schema/edm/initials> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Person>;
  rdfs:comment "The initials of a Person.";
  rdfs:label "initials";
  rdfs:range <http://www.w3.org/2001/XMLSchema#string>;
  rdfs:subPropertyOf <https://data.elsevier.com/schema/edm/name> .

<https://data.elsevier.com/schema/edm/isbn> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Book>;
  rdfs:comment "The isbn of a Book. Note: If the entity is a Chapter use edm:partOfISBN to store the ISBN of the parent Book. This attribute is used to provide the identifier of the entity.";
  rdfs:label "isbn";
  rdfs:range <http://www.w3.org/2001/XMLSchema#string>;
  rdfs:subPropertyOf <https://data.elsevier.com/schema/edm/identifier> .

<https://data.elsevier.com/schema/edm/Book> a <http://www.w3.org/2002/07/owl#Class>;
  rdfs:comment "A Work containing a detailed exploration of a particular subject. Typically a long form Work containing a sequence of Chapters. Will be assigned an ISBN identifier.";
  rdfs:label "Book";
  rdfs:subClassOf <https://data.elsevier.com/schema/edm/Work> .

<https://data.elsevier.com/schema/edm/issn> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Journal>;
  rdfs:comment "The issn of a Journal. Note: If the entity is an Article use edm:partOfISSN to store the ISSN of the parent Journal. This attribute is used to provide the identifier of the entity.";
  rdfs:label "issn";
  rdfs:range <http://www.w3.org/2001/XMLSchema#string>;
  rdfs:subPropertyOf <https://data.elsevier.com/schema/edm/identifier> .

<https://data.elsevier.com/schema/edm/Journal> a <http://www.w3.org/2002/07/owl#Class>;
  rdfs:comment "A periodical where the primary content is scholarly articles. It behaves as a container for Articles.";
  rdfs:label "Journal";
  rdfs:subClassOf <https://data.elsevier.com/schema/edm/Work> .

<https://data.elsevier.com/schema/edm/mayInteractWith> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Drug>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Drug>;
  rdfs:comment "The Drug may interact with another Drug.";
  rdfs:label "may interact with";
  <http://www.w3.org/2002/07/owl#inverseOf> <https://data.elsevier.com/schema/edm/mayInteractWith> .

<https://data.elsevier.com/schema/edm/mentionedIn> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Thing>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Work>;
  rdfs:comment "The entity is mentioned in the Work. For example an Experiment is described in an Article.";
  rdfs:label "mentioned in";
  <http://www.w3.org/2002/07/owl#inverseOf> <https://data.elsevier.com/schema/edm/mentions> .

<https://data.elsevier.com/schema/edm/mentions> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Work>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Thing>;
  rdfs:comment "he Work mentions the entity. For example an Article describes an Experiment.";
  rdfs:label "mentions" .

<https://data.elsevier.com/schema/edm/offeredBy> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Product>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Actor>;
  rdfs:comment "The Product is offered by an Actor.";
  rdfs:label "offered by";
  <http://www.w3.org/2002/07/owl#inverseOf> <https://data.elsevier.com/schema/edm/offers> .

<https://data.elsevier.com/schema/edm/offers> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Actor>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Product>;
  rdfs:comment "The Actor offers the Product.";
  rdfs:label "offers" .

<https://data.elsevier.com/schema/edm/orcid> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Person>;
  rdfs:comment "The orcid id of a Person.";
  rdfs:label "orcid";
  rdfs:range <http://www.w3.org/2001/XMLSchema#string>;
  rdfs:subPropertyOf <https://data.elsevier.com/schema/edm/identifier> .

<https://data.elsevier.com/schema/edm/originated> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Organization>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Work>;
  rdfs:comment "The Organization is the origin (through people creating or authoring) of a Work.";
  rdfs:label "originated";
  <http://www.w3.org/2002/07/owl#inverseOf> <https://data.elsevier.com/schema/edm/originatedAt> .

<https://data.elsevier.com/schema/edm/originatedAt> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Work>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Organization>;
  rdfs:comment "The Work is originated at an Organization (through people creating or authoring).";
  rdfs:label "originated at" .

<https://data.elsevier.com/schema/edm/pii> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Work>;
  rdfs:comment "Elsevier specific identifier used for Works such as Article, Chapter and ConferenceContribution published by Elsevier.";
  rdfs:label "pii";
  rdfs:range <http://www.w3.org/2001/XMLSchema#string>;
  rdfs:subPropertyOf <https://data.elsevier.com/schema/edm/identifier> .

<https://data.elsevier.com/schema/edm/preferredLabel> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Thing>;
  rdfs:comment "The preferred label for an entity.";
  rdfs:label "preferred label";
  rdfs:range <http://www.w3.org/2001/XMLSchema#string>;
  rdfs:subPropertyOf <https://data.elsevier.com/schema/edm/label> .

<https://data.elsevier.com/schema/edm/preferredName> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Actor>;
  rdfs:comment "The preferred name for an Actor, notably an Organization or a Person.";
  rdfs:label "preferred name";
  rdfs:range <http://www.w3.org/2001/XMLSchema#string>;
  rdfs:subPropertyOf <https://data.elsevier.com/schema/edm/name> .

<https://data.elsevier.com/schema/edm/providedBy> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Service>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Actor>;
  rdfs:comment "The Service is provided by an Actor.";
  rdfs:label "provided by";
  <http://www.w3.org/2002/07/owl#inverseOf> <https://data.elsevier.com/schema/edm/provides> .

<https://data.elsevier.com/schema/edm/provides> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Actor>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Service>;
  rdfs:comment "The Actor provides a Service." .

<https://data.elsevier.com/schema/edm/pui> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Work>;
  rdfs:comment "The identifier associated with the Work such as an Article, Chapter or ConferenceContribution in a form that doesn't include the main text of the Work. This identifier is used within Elsevier.";
  rdfs:label "pui";
  rdfs:range <http://www.w3.org/2001/XMLSchema#string>;
  rdfs:subPropertyOf <https://data.elsevier.com/schema/edm/identifier> .

<https://data.elsevier.com/schema/edm/purchasedBy> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Product>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Organization>,
    <https://data.elsevier.com/schema/edm/Person>;
  rdfs:comment "The Product is purchased by an Organization or Person.";
  rdfs:label "purchased by";
  <http://www.w3.org/2002/07/owl#inverseOf> <https://data.elsevier.com/schema/edm/purchases> .

<https://data.elsevier.com/schema/edm/purchases> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Organization>,
    <https://data.elsevier.com/schema/edm/Person>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Product>;
  rdfs:comment "The Organization or Person purchases a Product.";
  rdfs:label "purchases" .

<https://data.elsevier.com/schema/edm/referencedBy> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Work>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Work>;
  rdfs:comment "The Work is referenced by another Work.";
  rdfs:label "referenced by";
  <http://www.w3.org/2002/07/owl#inverseOf> <https://data.elsevier.com/schema/edm/references> .

<https://data.elsevier.com/schema/edm/references> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Work>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Work>;
  rdfs:comment "The Work references another Work.";
  rdfs:label "references" .

<https://data.elsevier.com/schema/edm/relatedTo> a <http://www.w3.org/2002/07/owl#ObjectProperty>,
    <http://www.w3.org/2002/07/owl#SymmetricProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Thing>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Thing>;
  rdfs:comment "Generic relationship that exists between two entities.";
  rdfs:label "related to";
  <http://www.w3.org/2002/07/owl#inverseOf> <https://data.elsevier.com/schema/edm/relatedTo> .

<https://data.elsevier.com/schema/edm/releaseDate> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Product>;
  rdfs:comment "The release date of a Product or product model.";
  rdfs:label "release date";
  rdfs:range <http://www.w3.org/2001/XMLSchema#dateTime>;
  rdfs:subPropertyOf <https://data.elsevier.com/schema/edm/date> .

<https://data.elsevier.com/schema/edm/ringgoldIdentifier> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Organization>;
  rdfs:comment "The identifier of an organization in Ringgold.";
  rdfs:label "ringgold identifier";
  rdfs:range <http://www.w3.org/2001/XMLSchema#string>;
  rdfs:subPropertyOf <https://data.elsevier.com/schema/edm/identifier> .

<https://data.elsevier.com/schema/edm/startDate> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Activity>,
    <https://data.elsevier.com/schema/edm/Agreement>, <https://data.elsevier.com/schema/edm/Relationship>,
    <https://data.elsevier.com/schema/edm/Role>;
  rdfs:comment "The start date associated with the entity.";
  rdfs:label "start date";
  rdfs:range <http://www.w3.org/2001/XMLSchema#dateTime>;
  rdfs:subPropertyOf <https://data.elsevier.com/schema/edm/date> .

<https://data.elsevier.com/schema/edm/targetedBy> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/DrugTarget>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Drug>;
  rdfs:comment "The DrugTarget is targeted by the Drug.";
  rdfs:label "targeted by";
  <http://www.w3.org/2002/07/owl#inverseOf> <https://data.elsevier.com/schema/edm/targets> .

<https://data.elsevier.com/schema/edm/DrugTarget> a <http://www.w3.org/2002/07/owl#Class>;
  rdfs:comment "Anything within a living organism to which a drug is directed or binds to resulting in a change in its behavior or function.";
  rdfs:label "Drug Target";
  rdfs:subClassOf <https://data.elsevier.com/schema/edm/Thing> .

<https://data.elsevier.com/schema/edm/targets> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Drug>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/DrugTarget>;
  rdfs:comment "The Drug targets to the DrugTarget.";
  rdfs:label "targets" .

<https://data.elsevier.com/schema/edm/title> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Work>;
  rdfs:comment "The title of a Work such as a Book or a composition. Not to be confused with the honorific of a Person.";
  rdfs:label "title";
  rdfs:range <http://www.w3.org/2001/XMLSchema#string>;
  rdfs:subPropertyOf <https://data.elsevier.com/schema/edm/label> .

<https://data.elsevier.com/schema/edm/trialOf> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Drug>, <https://data.elsevier.com/schema/edm/MedicalDevice>,
    <https://data.elsevier.com/schema/edm/MedicalProcedure>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/ClinicalTrial>;
  rdfs:comment "The effect of the MedicalDevice, Drug or MedicalProcedure is studied in the ClinicalTrial.";
  rdfs:label "trial of";
  <http://www.w3.org/2002/07/owl#inverseOf> <https://data.elsevier.com/schema/edm/trialed> .

<https://data.elsevier.com/schema/edm/ClinicalTrial> a <http://www.w3.org/2002/07/owl#Class>;
  rdfs:comment "Experiments performed in clinical research including trials of drugs, devices and procedures often regulated and requiring consent.";
