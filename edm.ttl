  rdfs:subClassOf <https://data.elsevier.com/schema/edm/BiomedicalResearch> .

<https://data.elsevier.com/schema/edm/BiomedicalResearch> a <http://www.w3.org/2002/07/owl#Class>;
  rdfs:comment "Research studies and experiments relating to human and veterinary medicine or health, including lab experiments, observational studies and interventional trials and registries, randomized, controlled or not.";
  rdfs:label "Biomedical Research";
  rdfs:subClassOf <https://data.elsevier.com/schema/edm/Experiment> .

<https://data.elsevier.com/schema/edm/Experiment> a <http://www.w3.org/2002/07/owl#Class>;
  rdfs:comment "An experiment is a procedure carried out to support, refute, or validate a hypothesis or in order to investigate and explore phenomena.";
  rdfs:label "Experiment";
  rdfs:subClassOf <https://data.elsevier.com/schema/edm/Activity> .

<https://data.elsevier.com/schema/edm/trialed> a <http://www.w3.org/2002/07/owl#ObjectProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/ClinicalTrial>;
  <http://schema.org/rangeIncludes> <https://data.elsevier.com/schema/edm/Drug>, <https://data.elsevier.com/schema/edm/MedicalDevice>,
    <https://data.elsevier.com/schema/edm/MedicalProcedure>;
  rdfs:comment "The ClinicalTrial is conducted to study the effect of a MedicalDevice, Drug or MedicalProcedure.";
  rdfs:label "trialed" .

<http://schema.org/inverseOf> <http://schema.org/domainIncludes> <http://schema.org/Property>;
  <http://schema.org/isPartOf> <http://meta.schema.org/>;
  <http://schema.org/rangeIncludes> <http://schema.org/Property>;
  rdfs:comment "Relates a property to a property that is its inverse. Inverse properties relate the same pairs of items to each other, but in reversed direction. For example, the 'alumni' and 'alumniOf' properties are inverseOf each other. Some properties don't have explicit inverses; in these situations RDFa and JSON-LD syntax for reverse properties can be used.";
  rdfs:label "inverseOf" .

<http://schema.org/supersededBy> <http://schema.org/domainIncludes> <http://schema.org/Class>,
    <http://schema.org/Enumeration>, <http://schema.org/Property>;
  <http://schema.org/isPartOf> <http://meta.schema.org/>;
  <http://schema.org/rangeIncludes> <http://schema.org/Class>, <http://schema.org/Enumeration>,
    <http://schema.org/Property>;
  rdfs:comment "Relates a term (i.e. a property, class or enumeration) to one that supersedes it.";
  rdfs:label "supersededBy" .

<https://data.elsevier.com/schema/edm/addressCountry> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/PostalAddress>;
  rdfs:comment """The country used in postal addresses.
For other uses of country please use edm:country""";
  rdfs:label "address country";
  rdfs:subPropertyOf <https://data.elsevier.com/schema/edm/locationLabel> .

<https://data.elsevier.com/schema/edm/PostalAddress> a <http://www.w3.org/2002/07/owl#Class>;
  rdfs:comment "A collection of information such as city and street name used to describe the location associated with an entity.";
  rdfs:label "Postal Address";
  rdfs:subClassOf <https://data.elsevier.com/schema/edm/Location> .

<https://data.elsevier.com/schema/edm/Location> a <http://www.w3.org/2002/07/owl#Class>;
  rdfs:comment "The geographic location expressed as a set of coordinates.";
  rdfs:label "Location";
  rdfs:subClassOf <https://data.elsevier.com/schema/edm/Thing> .

<https://data.elsevier.com/schema/edm/locationLabel> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Activity>;
  rdfs:comment """The location as a piece of text where the Activity is held.
Use the Location entity type when coordinates are separated out. See attributes that are linked with Location.""";
  rdfs:label "location label" .

<https://data.elsevier.com/schema/edm/addressLocality> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/PostalAddress>;
  rdfs:comment "The locality such as the settlement name.";
  rdfs:label "address locality";
  rdfs:subPropertyOf <https://data.elsevier.com/schema/edm/locationLabel> .

<https://data.elsevier.com/schema/edm/addressRegion> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/PostalAddress>;
  rdfs:comment "The administrative region or area used in postal addresses.";
  rdfs:label "address region";
  rdfs:subPropertyOf <https://data.elsevier.com/schema/edm/locationLabel> .

<https://data.elsevier.com/schema/edm/altitude> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Location>;
  rdfs:comment "The height of a Thing above a reference level, especially above sea level or above the earth's surface.";
  rdfs:label "altitude";
  rdfs:subPropertyOf <https://data.elsevier.com/schema/edm/locationLabel> .

<https://data.elsevier.com/schema/edm/casNumber> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Substance>;
  rdfs:comment "A numerical identifier assigned by the Chemical Abstracts Service (CAS) to every chemical substance described in the open scientific literature.";
  rdfs:label "CAS number";
  rdfs:subPropertyOf <https://data.elsevier.com/schema/edm/identifier> .

<https://data.elsevier.com/schema/edm/country> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Thing>;
  rdfs:comment """The country associated with the entity.
For postal addresses use edm:addressCountry.""";
  rdfs:label "country";
  rdfs:subPropertyOf <https://data.elsevier.com/schema/edm/locationLabel> .

<https://data.elsevier.com/schema/edm/createdDate> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Work>;
  rdfs:comment "The date or time the Work was created.";
  rdfs:label "created date";
  rdfs:subPropertyOf <https://data.elsevier.com/schema/edm/date> .

<https://data.elsevier.com/schema/edm/edition> a <http://www.w3.org/2002/07/owl#DatatypeProperty>;
  <http://schema.org/domainIncludes> <https://data.elsevier.com/schema/edm/Book>;
  rdfs:comment "The edition of the Book.";
